snippet header "Header comment"
" File: ${1:`!v expand('%:p:h:t').'/'.expand('%:t')`}
" Author: ${2:`!v g:snips_author`}
" Description: ${3}
${4:" Last Change: `!v strftime("%b %d, %Y")`}
endsnippet

snippet save_cpo
let s:save_cpo = &cpo
set cpo&vim

${1}

let &cpo = s:save_cpo
unlet s:save_cpo
endsnippet
